        @import "https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700";

        @mixin transition($settings) {
          -webkit-transition: $settings;
          -moz-transition: $settings;
          -ms-transition: $settings;
          -o-transition: $settings;
          transition: $settings;
        }
        
        .editinfo{
            display: none;
        }

        .list-groupp{
          margin-top:2%;
          cursor:pointer;
        }

        .list-group-iteem{
          padding:1%;
          padding-right:2%;
          padding-left: 7%;
          min-height:10px;
          background: white;
          border-radius: 20px;
          box-sizing: border-box;
          border: 2px solid #6D7FCC;
          text-width:bold;

        }

        .list-group-iteem hr{
          margin-top:2px;
          margin-bottom:10px;

        }

        .hide{
          font-size:12px;
          padding-bottom:10px;
        }

        .hiddd{
          display:none;
        }

        #Feedback i{
          float: right;
        }
        .btn-collapsible {
          overflow: hidden;
          @include transition(all 500ms ease-in-out);
          max-width: 30px;
          &.btn-xs {
            max-width: 25px;
          }
          &
          &.btn-lg {
            max-width: 50px;
          }
        }

        .card i{
          width:30px;
        }

        .btn-sm {
           height: 30px;
           padding-top:2px;
           padding-left : 7px;
        }
        .card span {
          opacity: 1;
          padding-left:9px;
          display: inline-block;
          @include transition(all 500ms ease-in-out);
        }

        .btn-collapsible:hover {
          max-width: 300px;
          @include transition(all 300ms ease-in-out);
        }



        .browser-default{
          width: 200px;
          position: absolute;
          right: 120px;
        }
        
        .outercard{
            padding:1.5%;
            padding-bottom:1%;
            float:left;
        }

        .card{
          box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);
          
        }

        .card-body{
          padding:0.50em;
        }

        .card-body li{
          margin-top:0;
          font-size: .9rem;
        }
        
        .card img{
          height: 230px;

        }
        
        .litype{
            height:40px;
            line-height: 18px;
